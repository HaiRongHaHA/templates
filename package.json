{
  "name": "template-vue3",
  "version": "0.1.0",
  "private": true,
  "keywords": [
    "vue-cli",
    "vue3",
    "vue-router",
    "setup",
    "typescript",
    "pinia",
    "template"
  ],
  "engines": {
    "node": "^12 || >=14"
  },
  "scripts": {
    "serve": "cross-env VUE_ENV=dev vue-cli-service serve",
    "build:dev": "cross-env VUE_ENV=dev vue-cli-service build --mode production",
    "build:prod": "cross-env VUE_ENV=prod vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "lint:eslint": "eslint src/**/*.{vue,ts,tsx} --fix",
    "lint:prettier": "prettier --write  src/**/*.{js,json,ts,tsx,scss,vue}",
    "lint:lint-staged": "lint-staged",
    "commit": "git-cz"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "axios-retry": "^3.2.4",
    "core-js": "^3.6.5",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.12",
    "qs": "^6.10.3",
    "vant": "^3.4.3",
    "vconsole": "^3.14.3",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-plugin-router": "~4.5.13",
    "@vue/cli-plugin-typescript": "~4.5.13",
    "@vue/cli-plugin-vuex": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.2.4",
    "commitlint": "^16.2.3",
    "cross-env": "^7.0.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "postcss-pxtorem": "5.1.1",
    "prettier": "^2.6.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-resources-loader": "^1.5.0",
    "stylelint": "13.13.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-scss": "3.19.0",
    "typescript": "~4.1.5",
    "vue-cli-plugin-style-resources-loader": "^0.1.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{js,json,ts,tsx,vue}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{scss,vue}": [
      "prettier --write",
      "stylelint --fix"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
